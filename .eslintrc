{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
  },
  "rules": {
    "new-cap": ["error", { "capIsNewExceptions": ["Router"] }],
    "global-require": 0,
    "no-underscore-dangle": 0,
    "import/no-dynamic-require": 0,
  },
  "plugins": [
    "react", "import"
  ],
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolve": {
      "moduleDirectory": ["node_modules", "src"]
    }
  },
  "globals": {
  }
}
